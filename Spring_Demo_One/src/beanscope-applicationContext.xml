<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
						http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">
	<context:property-placeholder location="classpath:info.properties"/>
	<bean id="myFortuneService"
		class="com.java.springdemo.HAppyFortuneService">
		<property name="name" value="${foo.name}"></property>
		<property name="number" value="${foo.number}"></property>
	</bean>
	<bean id="myBaseballCoach"
		class="com.java.springdemo.BaseballCoach"
		scope="prototype">
		<!-- <constructor-arg ref="myFortuneService"></constructor-arg>-->
		<property name="fortuneService" ref="myFortuneService"></property>
	</bean>
</beans>